{
  "dashboard": {
    "title": "Java Serialization Frameworks - Performance Overview",
    "tags": ["serialization", "benchmark", "performance"],
    "timezone": "browser",
    "schemaVersion": 38,
    "version": 1,
    "refresh": "30s",
    "panels": [
      {
        "id": 1,
        "title": "Serialization Time by Framework",
        "type": "timeseries",
        "gridPos": {"h": 8, "w": 12, "x": 0, "y": 0},
        "targets": [
          {
            "expr": "serialization_time_ms",
            "legendFormat": "{{framework}} - {{scenario}}",
            "refId": "A"
          }
        ],
        "fieldConfig": {
          "defaults": {
            "unit": "ms",
            "custom": {
              "drawStyle": "line",
              "lineInterpolation": "smooth",
              "showPoints": "auto"
            }
          }
        },
        "options": {
          "legend": {
            "displayMode": "table",
            "placement": "right",
            "calcs": ["mean", "min", "max"]
          },
          "tooltip": {"mode": "multi"}
        }
      },
      {
        "id": 2,
        "title": "Throughput (ops/sec) by Framework",
        "type": "timeseries",
        "gridPos": {"h": 8, "w": 12, "x": 12, "y": 0},
        "targets": [
          {
            "expr": "serialization_throughput_ops",
            "legendFormat": "{{framework}} - {{scenario}}",
            "refId": "A"
          }
        ],
        "fieldConfig": {
          "defaults": {
            "unit": "ops",
            "custom": {
              "drawStyle": "line",
              "lineInterpolation": "smooth"
            }
          }
        }
      },
      {
        "id": 3,
        "title": "Payload Size Comparison",
        "type": "bargauge",
        "gridPos": {"h": 8, "w": 12, "x": 0, "y": 8},
        "targets": [
          {
            "expr": "payload_size_bytes",
            "legendFormat": "{{framework}}",
            "refId": "A"
          }
        ],
        "fieldConfig": {
          "defaults": {
            "unit": "bytes",
            "mappings": [],
            "thresholds": {
              "mode": "absolute",
              "steps": [
                {"value": null, "color": "green"},
                {"value": 10000, "color": "yellow"},
                {"value": 100000, "color": "red"}
              ]
            }
          }
        },
        "options": {
          "orientation": "horizontal",
          "displayMode": "gradient",
          "showUnfilled": true
        }
      },
      {
        "id": 4,
        "title": "Compression Ratio",
        "type": "stat",
        "gridPos": {"h": 8, "w": 12, "x": 12, "y": 8},
        "targets": [
          {
            "expr": "compression_ratio{config=\"with_compression\"}",
            "legendFormat": "{{framework}}",
            "refId": "A"
          }
        ],
        "fieldConfig": {
          "defaults": {
            "unit": "percentunit",
            "mappings": [],
            "thresholds": {
              "mode": "absolute",
              "steps": [
                {"value": null, "color": "red"},
                {"value": 0.5, "color": "yellow"},
                {"value": 0.3, "color": "green"}
              ]
            }
          }
        },
        "options": {
          "reduceOptions": {
            "values": false,
            "calcs": ["lastNotNull"]
          },
          "orientation": "auto",
          "textMode": "value_and_name"
        }
      },
      {
        "id": 5,
        "title": "Memory Usage by Framework",
        "type": "timeseries",
        "gridPos": {"h": 8, "w": 12, "x": 0, "y": 16},
        "targets": [
          {
            "expr": "memory_usage_mb",
            "legendFormat": "{{framework}}",
            "refId": "A"
          }
        ],
        "fieldConfig": {
          "defaults": {
            "unit": "mbytes",
            "custom": {
              "drawStyle": "line",
              "fillOpacity": 20
            }
          }
        }
      },
      {
        "id": 6,
        "title": "CPU Usage",
        "type": "timeseries",
        "gridPos": {"h": 8, "w": 12, "x": 12, "y": 16},
        "targets": [
          {
            "expr": "cpu_usage_percent",
            "legendFormat": "{{framework}}",
            "refId": "A"
          }
        ],
        "fieldConfig": {
          "defaults": {
            "unit": "percent",
            "max": 100,
            "custom": {
              "drawStyle": "line",
              "fillOpacity": 10
            }
          }
        }
      },
      {
        "id": 7,
        "title": "Performance by Payload Size",
        "type": "heatmap",
        "gridPos": {"h": 8, "w": 24, "x": 0, "y": 24},
        "targets": [
          {
            "expr": "serialization_time_ms",
            "refId": "A"
          }
        ],
        "options": {
          "calculate": true,
          "calculation": {
            "xBuckets": {
              "mode": "size",
              "value": "1"
            }
          },
          "color": {
            "mode": "scheme",
            "scheme": "Spectral"
          },
          "yAxis": {
            "unit": "ms"
          }
        }
      },
      {
        "id": 8,
        "title": "Best Framework by Category",
        "type": "table",
        "gridPos": {"h": 8, "w": 24, "x": 0, "y": 32},
        "targets": [
          {
            "expr": "serialization_time_ms",
            "format": "table",
            "refId": "A"
          }
        ],
        "transformations": [
          {
            "id": "groupBy",
            "options": {
              "fields": {
                "framework": {
                  "aggregations": [],
                  "operation": "groupby"
                },
                "Value": {
                  "aggregations": ["mean", "min"],
                  "operation": "aggregate"
                }
              }
            }
          },
          {
            "id": "sortBy",
            "options": {
              "fields": {},
              "sort": [
                {
                  "field": "Value (mean)",
                  "desc": false
                }
              ]
            }
          }
        ],
        "options": {
          "showHeader": true,
          "sortBy": [
            {
              "displayName": "Value (mean)",
              "desc": false
            }
          ]
        }
      }
    ],
    "templating": {
      "list": [
        {
          "name": "scenario",
          "type": "query",
          "label": "Payload Size",
          "query": "label_values(serialization_time_ms, scenario)",
          "multi": true,
          "includeAll": true,
          "allValue": ".*"
        },
        {
          "name": "framework",
          "type": "query",
          "label": "Framework",
          "query": "label_values(serialization_time_ms, framework)",
          "multi": true,
          "includeAll": true,
          "allValue": ".*"
        },
        {
          "name": "config",
          "type": "query",
          "label": "Configuration",
          "query": "label_values(serialization_time_ms, config)",
          "multi": false,
          "includeAll": false
        }
      ]
    },
    "annotations": {
      "list": [
        {
          "name": "Benchmark Runs",
          "datasource": "-- Grafana --",
          "enable": true,
          "iconColor": "blue"
        }
      ]
    }
  }
}
